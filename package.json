{
  "name": "carousel-vue3-ts",
  "version": "1.0.2",
  "description": "a Vue3 carousel component.",
  "author": "Emad Abdelmaksoud <e.abdellmaksoud@gmail.com>",
  "homepage": "https://github.com/emadadel999/carousel#readme",
  "main": "./dist/carousel-vue3-ts.js",
  "module": "./dist/carousel-vue3-ts.umd.cjs",
  "types": "./dist_types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist_types/index.d.ts",
        "default": "./dist/carousel-vue3-ts.js"
      },
      "require": "./dist/carousel-vue3-ts.umd.cjs",
      "style": "./dist/style.css"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@tsconfig/node18": "^18.2.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.64.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vue-tsc": "^1.8.6"
  }
}
